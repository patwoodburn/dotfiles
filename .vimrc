set nocompatible 
set nu
set ruler
syntax enable
set expandtab
set tabstop=2 shiftwidth=2 softtabstop=2
set autoindent
set showcmd

set splitright
set splitbelow

nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>

set t_Co=256
color dracula
set colorcolumn=80
